# AI Service Configuration
# Copy this file to .env and replace with your actual API keys

# Groq API (Recommended - Fast and reliable)
# Get your free API key from: https://console.groq.com/keys
VITE_GROQ_API_KEY=your_groq_api_key_here

# OpenAI API (Optional - for GPT models)
# Get your API key from: https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Hugging Face API (Optional - for open source models)
# Get your API key from: https://huggingface.co/settings/tokens
VITE_HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Default AI Provider (groq, openai, or huggingface)
VITE_DEFAULT_AI_PROVIDER=groq

# App Configuration
VITE_APP_NAME=Smart Finance Assistant
VITE_APP_VERSION=2.0.0

# Currency Settings
VITE_DEFAULT_CURRENCY=INR
VITE_CURRENCY_SYMBOL=â‚¹

# Feature Flags
VITE_ENABLE_AI_ANALYSIS=true
VITE_ENABLE_EXPORT=true
VITE_ENABLE_GOALS=true

# Development Settings
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info